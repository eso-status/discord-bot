{
  "name": "@eso-status/discord-bot",
  "version": "1.0.0-dev.0",
  "description": "Eso-status discord bot",
  "author": "@dov118 <contact@dov118.dev> (https://dov118.dev)",
  "private": true,
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": "20.16.0"
  },
  "bugs": {
    "url": "https://github.com/eso-status/discord-bot/issues"
  },
  "homepage": "https://github.com/eso-status/discord-bot#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:eso-status/discord-bot.git"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --setupFiles",
    "test:watch": "jest --runInBand --watch --setupFiles",
    "test:cov": "jest --runInBand --coverage --setupFiles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --setupFiles",
    "test:e2e": "jest --runInBand --config ./test/jest-e2e.json --setupFiles",
    "migration:run": "typeorm migration:run -d ./dist/config/typeorm.config.js"
  },
  "dependencies": {
    "@discord-nestjs/common": "^5.2.12",
    "@discord-nestjs/core": "^5.3.14",
    "@eso-status/connector": "^2.0.0",
    "@eso-status/forum-message": "1.0.10",
    "@eso-status/live-services": "1.0.6",
    "@eso-status/service-alerts": "1.0.7",
    "@eso-status/types": "1.0.6",
    "@nestjs/cli": "10.4.2",
    "@nestjs/common": "10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "10.3.10",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/mapped-types": "2.0.5",
    "@nestjs/platform-express": "10.3.10",
    "@nestjs/platform-socket.io": "10.3.10",
    "@nestjs/schedule": "4.1.0",
    "@nestjs/typeorm": "10.0.2",
    "@nestjs/websockets": "10.3.10",
    "aws-sdk": "2.1667.0",
    "discord.js": "^14.15.3",
    "dotenv": "16.4.5",
    "moment": "2.30.1",
    "mysql2": "3.11.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "socket.io": "4.7.5",
    "socket.io-client": "4.7.5",
    "sqlite3": "5.1.7",
    "supertest": "7.0.0",
    "typeorm": "0.3.20",
    "typeorm-extension": "3.6.0",
    "winston": "3.13.1",
    "winston-cloudwatch": "6.3.0",
    "necord": "^6.8.3",
    "@nestjs/testing": "^10.3.10",
    "ts-mockito": "^2.6.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "10.1.3",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "22.1.0",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-sonarjs": "1.0.4",
    "eslint-plugin-unused-imports": "3.2.0",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.0.0",
    "source-map-support": "0.5.21",
    "ts-jest": "29.2.4",
    "ts-loader": "9.5.1",

    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.e2e-spec\\.ts|.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "dotenv/config"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
